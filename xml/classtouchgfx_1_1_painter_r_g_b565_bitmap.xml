        <type>void</type>
        <definition>virtual void render</definition>
        <argsstring>(uint8_t *ptr, int x, int xAdjust, int y, unsigned count, const uint8_t *covers)</argsstring>
        <name>render</name>
        <reimplements refid="classtouchgfx_1_1_abstract_painter_r_g_b565_1a8441245669080e102adac555a07830ea">render</reimplements>
        <param>
          <type>uint8_t *</type>
          <declname>ptr</declname>
        </param>
        <param>
          <type>int</type>
          <declname>x</declname>
        </param>
        <param>
